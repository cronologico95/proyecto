<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.phonegap.example" versionCode="10" version="1.0.0">
    <!-- versionCode is optional and Android only -->
    <name>PhoneGap Example</name>
    <description> An example for phonegap build docs. </description>
    <author href="https://build.phonegap.com" email="support@phonegap.com"> Hardeep Shoker </author>
    <preference name="Orientation" value="portrait" />
	<preference name="phonegap-version" value="cli-6.5.0" />
    <feature name="http://api.phonegap.com/1.0/network" />
    <access origin="*" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" /> </widget>